                    <li class="nav-item"> <a href="/posts/<%= data.email %>" class="navbar-brand">Blog Page</a> </li>


                    <li class="nav-item"> <a href="/" class="navbar-brand">Logout</a> </li>


                </ul>

            </div>
        </nav>
        <header>
            <div id="indicators" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                    <li data-target="#indicators" data-slide-to="0" class="active"></li>
                    <li data-target="#indicators" data-slide-to="1"></li>
                    <li data-target="#indicators" data-slide-to="2"></li>
                </ol>
                <div class="carousel-inner" role="listbox">

                    <div class="carousel-item active" style="background-image : url('https://images.pexels.com/photos/1483024/pexels-photo-1483024.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940');">
                        <div class="carousel-caption d-md-block">
                            <h3 class="display-4">Trainsafar</h3>
                            <p class="lead">Your Travel Buddy</p>
                        </div>
                    </div>
                    <div class="carousel-item" style="background-image : url('https://images.pexels.com/photos/672358/pexels-photo-672358.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940');">
                        <div class="carousel-caption d-md-block">
                            <h3 class="display-4">Trainsafar</h3>
                            <p class="lead">Your Travel Buddy</p>
                        </div>
                    </div>
                    <div class="carousel-item" style="background-image : url('https://images.pexels.com/photos/512890/pexels-photo-512890.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940');">
                        <div class="carousel-caption d-md-block">
                            <h3 class="display-4">Trainsafar</h3>
                            <p class="lead">Your Travel Buddy</p>
                        </div>
                    </div>
                </div>
                <a class="carousel-control-prev" href="#indicators" role="button" data-slide="prev"> <span class="carousel-control-prev-icon" aria-hidden="true"></span> <span class="sr-only">Previous</span> </a>
                <a class="carousel-control-next" href="#indicators" role="button" data-slide="next"> <span class="carousel-control-next-icon" aria-hidden="true"></span> <span class="sr-only">Next</span> </a>
            </div>
        </header>
    </div>




    <div class="container">
        <div class="row m-4">
            <div class="col-md-4 col-sm-4">
                <div class="login-card-purple p-3 shadow-lg rounded">
                    <div class="pt-3">
                        <h2 class="text-white ">Search Trains</h2>
                    </div>

                    <div class="mt-5">
                        <div class="form-group">
                            <input type="text" placeholder="Source Staion Code" name="source" class="form-control form-control-sm bg-light" id="source" oninput="suggestStations('source')">
                            <ul name="" id="result" style="height:150px; width:80%; overflow:hidden; overflow-y:scroll; display: none;">

                            </ul>

                        </div>

                        <div class="form-group">

                            <input type="text" name="destination" id="destination" oninput="suggestStations('destination')" class="form-control form-control-sm bg-light" placeholder="Destination Station Code">

                        </div>

                        <div class="form-group">
                            <input type="date" name="date" id="date" class="form-control form-control-sm bg-light">

                        </div>

                        <div class="mt-5">
                            <button onclick="searchTrains()" class="btn btn-sm btn-light col" data-toggle="modal" data-target="#exampleModalLong">
                                    Search
                                </button>
                        </div>


                    </div>
                </div>
            </div>

            <div class="col-md-4 col-sm-4">
                <div class="login-card-light p-3 shadow-lg rounded">
                    <div class="pt-3">
                        <h2 class="text-danger">Check Live Train Status</h2>
                    </div>

                    <div class="mt-5">
                        <div class="form-group">
                            <input type="text" name="trainno" id="trainno" placeholder="Enter Train number/name:" class="form-control form-control-sm">

                        </div>

                        <div class="form-group">
                            <input type="date" id="date_live" class="form-control form-control-sm">

                        </div>

                        <div class="form-group">
                            <input type="text" class="form-control form-control-sm" placeholder="Select Station" id="code_live">

                        </div>

                        <div class="mt-5">
                            <button onclick="track()" class="btn btn-sm btn-danger col" data-toggle="modal" data-target="#exampleModalLong">
                                    Track
                                </button>
                        </div>




                    </div>
                </div>
            </div>

            <div class="col-md-4 col-sm-4">
                <div class="login-dark p-3 shadow-lg rounded">
                    <div class="pt-3">
                        <h2 class="text-white ">Check PNR status</h2>
                    </div>

                    <div class="mt-5">
                        <div class="form-group">
                            <input type="text" name="pnr" id="pnr" class="form-control form-control-sm bg-light" placeholder="Enter 10 digit PNR">
                        </div>



                        <div class="mt-5">
                            <button onclick="pnrStatus()" class="btn btn-sm btn-light col" data-toggle="modal" data-target="#exampleModalLong">
                                    Check
                                </button>
                        </div>


                    </div>
                </div>
            </div>
        </div>


    </div>

    <!-- Modal -->
